{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^5.0.12",
    "@umijs/plugin-esbuild": "^1.0.1",
    "@umijs/preset-react": "^1.6.8",
    "@umijs/test": "^3.2.22",
    "axios": "^0.20.0",
    "leaflet": "^1.7.1",
    "lint-staged": "^10.0.7",
    "mockjs": "^1.1.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "redux-logger": "^3.0.6",
    "umi": "^3.2.22",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@antv/g2": "^4.0.15",
    "classnames": "^2.2.6",
    "html2canvas": "^1.0.0-rc.7",
    "less": "^3.12.2",
    "lodash": "^4.17.20"
  }
}
